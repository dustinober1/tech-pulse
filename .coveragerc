[run]
source = .
omit =
    venv/*
    test/*
    test_results/*
    */site-packages/*
    __pycache__/*
    *.pyc
    .git/*

[report]
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod